{
  "theme": "ANSI",
  "contextFileName": [
    "AGENTS.md",
    "GEMINI.md"
  ],
  "selectedAuthType": "oauth-personal",
  "preferredEditor": "neovim",
  "mcpServers": {
    "postgres_docker": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "bytebase/dbhub",
        "--transport",
        "stdio",
        "--dsn",
        "postgres://${POSTGRES_USER}:${POSTGRES_PASSWORD}@host.docker.internal:${POSTGRES_PORT}/${POSTGRES_DB}"
      ],
      "env": {
        "POSTGRES_USER": "$POSTGRES_USER",
        "POSTGRES_PASSWORD": "$POSTGRES_PASSWORD",
        "POSTGRES_PORT": "$POSTGRES_PORT",
        "POSTGRES_DB": "$POSTGRES_DB"
      }
    }
  }
}
